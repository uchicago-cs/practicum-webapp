<h3>Set the current quarter</h3>
<%= form_for @quarter do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :season %>
  <%= f.text_field :season %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.submit "Set the current quarter",
  class: "btn btn-large btn-primary" %>
<% end %>

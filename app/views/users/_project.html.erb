<tr class="<%= advisor_project_row_class(project) %>">
  <td><%= link_to(project.name,
		  project_path(project, year: project.quarter.year,
			       season: project.quarter.season)) %></td>
  <td><%= formatted_quarter(project.quarter) %></td>
  <td><%= project.created_at.to_date %></td>
  <td><%= formatted_project_status(project) %></td>
  <td><%= formatted_submission_count(project) %></td>
  <td><%= project_notes(project) %></td>
</tr>

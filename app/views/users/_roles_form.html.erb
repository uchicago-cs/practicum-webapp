<%= form_for @user do |f| %>
  <%= render 'shared/error_messages', object: f.object %>


  <%= f.check_box :student %>
  <%= f.label :student, class: "inline" %> |

  <%= f.check_box :advisor %>
  <%= f.label :advisor, class: "inline" %> |

  <%= f.check_box :admin %>
  <%= f.label :admin, class: "inline" %>

  <%= f.submit "Change this user's roles",
  class: "btn btn-primary" %>
<% end %>
